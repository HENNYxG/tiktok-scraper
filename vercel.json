{
  "version": 2,
  "builds": [
    {
      "src": "api/scrape.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs16"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/scrape",
      "dest": "/api/scrape.js"
    }
  ],
  "functions": {
    "api/scrape.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
